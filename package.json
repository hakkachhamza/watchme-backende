
{
  "name": "watchme-admin-server",
  "version": "2.0.0",
  "description": "Watch Me Admin Panel Backend Server with Flutter App API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "db:init": "mysql -u root -p < watchme_db.sql",
    "db:reset": "mysql -u root -p -e 'DROP DATABASE IF EXISTS watchme_db; CREATE DATABASE watchme_db;' && npm run db:init",
    "docker:build": "docker build -t watchme-admin .",
    "docker:run": "docker run -p 5000:5000 watchme-admin"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.16.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "iptv",
    "admin-panel",
    "streaming",
    "subscription",
    "flutter-api"
  ],
  "author": "Watch Me Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/watchme-admin.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/watchme-admin/issues"
  },
  "homepage": "https://github.com/yourusername/watchme-admin#readme"
}
